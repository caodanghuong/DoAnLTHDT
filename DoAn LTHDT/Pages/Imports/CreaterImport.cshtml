@page
@model DoAn_LTHDT.Pages.Imports.CreaterImportModel
@{
}
<p>Please Enter Import Product Information</p>
<form method="Post">
	<label>Enter Product ID</label><br />
	<input type="number" name="createrImport" required/><br />

	<label>Enter Quantities</label><br />
	<input type="number" name="quantitiesImport" required /><br />

	<label>Enter Prices</label><br />
	<input type="number" name="priceImport" required /><br />


	<label>Enter Date Import </label><br />
	<label>Enter format YYYY/MM/DD</label><br />
	<input type="datetime" name="dateCheckImport" required /><br />


	<label>Enter Total</label><br />
	<input type="number" name="totalImport" required /><br />

	<input type="submit" value="Edit" /><br />
</form>
<p>@Model.result</p>