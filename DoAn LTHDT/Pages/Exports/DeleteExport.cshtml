@page
@model DoAn_LTHDT.Pages.Exports.DeleteExportModel
@{
}
<p>Export Information Delete</p>
<form method="Post">
	<label>Export ID Delete</label><br />
	<input type="number" name="IdExportDelete" value="@Model.ExportModel.GETEXPORTID()" readonly /><br />

	<label>Enter ID Product Delete</label><br />
	<input type="number" name="IDProductDelete" value="@Model.ExportModel.GETPRODUCTID()" readonly /><br />

	<label>Enter Quantities of Export Delete</label><br />
	<input type="number" name="quantitesDelete" value="@Model.ExportModel.GETQUANTITIES()" readonly /><br />

	<label>Enter Price of Export Delete</label><br />
	<input type="number" name="priceDelete" value="@Model.ExportModel.GETPRICE()" readonly /><br />

	<label>Enter Date Check of Export Delete</label><br />
	<label>Enter format YYYY/MM/DD</label><br />
	<input type="datetime" name="dateExpiredDelete" value="@Model.ExportModel.GETEXPORTDATE()" readonly /><br />


	<label>Enter Total of Export Delete</label><br />
	<input type="number" name="totalDelete" value="@Model.ExportModel.GETTOTAL()" readonly /><br />



	<input type="submit" value="Delete" /><br />
</form>
<p>@Model.result</p>